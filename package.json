{
  "name": "aws_dev_hour",
  "version": "0.1.0",
  "type": "module",
  "bin": {
    "aws_dev_hour": "bin/aws_dev_hour.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "find ./ -type f -name \\*.spec.ts -not -path './e2e-tests/*' -not -path './node_modules/*' -not -path './dist/*' | xargs npx tsx --test --test-reporter spec",
    "test:e2e": "npx tsx --test e2e-tests/getLabels.spec.ts",
    "cdk": "cdk",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lenakh97/serverlessAWSTraining.git"
  },
  "devDependencies": {
    "@aws-sdk/client-cloudformation": "3.526.0",
    "@aws-sdk/client-cognito-identity-provider": "3.525.0",
    "@aws-sdk/client-dynamodb": "3.525.0",
    "@aws-sdk/client-iam": "3.525.0",
    "@aws-sdk/client-rekognition": "3.525.0",
    "@aws-sdk/client-s3": "3.525.0",
    "@aws-sdk/lib-dynamodb": "3.525.0",
    "@nordicsemiconductor/asset-tracker-cloud-code-style": "13.1.16",
    "@nordicsemiconductor/cloudformation-helpers": "9.0.3",
    "@types/aws-lambda": "8.10.136",
    "@types/node": "20.11.24",
    "@types/prettier": "3.0.0",
    "aws-cdk": "2.131.0",
    "aws-cdk-lib": "2.131.0",
    "constructs": "10.3.0",
    "husky": "9.0.11",
    "@nordicsemiconductor/eslint-config-asset-tracker-cloud-typescript": "17.0.0",
    "@typescript-eslint/eslint-plugin": "7.1.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-unicorn": "51.0.1",
    "lint-staged": "15.2.2",
    "p-retry": "6.2.0",
    "prettier": "3.2.5",
    "tsmatchers": "5.0.2",
    "tsx": "4.7.1",
    "typescript": "5.3.3"
  },
  "dependencies": {
    "@nordicsemiconductor/from-env": "3.0.1",
    "sharp": "0.33.2"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --ext .js,.ts,.jsx,.tsx"
    ],
    "*.{md,json,yaml,yml,html}": [
      "prettier --write"
    ]
  },
  "release": {
    "branches": [
      "saga"
    ],
    "remoteTags": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/github",
        {
          "successComment": false,
          "failTitle": false
        }
      ]
    ]
  },
  "prettier": "@nordicsemiconductor/asset-tracker-cloud-code-style/.prettierrc"
}
